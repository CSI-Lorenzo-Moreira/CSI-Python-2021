	function OGfindMoreImages(){
		
		var i = $('body').find('img');
		var docHead = ('head');
		var base = 'https://www.sanignacio.pr';
		
		$(i).each(function(){
			
			var imgSrc = $(this).attr('src');
			if (imgSrc.indexOf("http") >= 0) {
				$imgSrcComplete = imgSrc;
			} else {
				$imgSrcComplete = base+imgSrc;
			}
			
			$(docHead).append('<meta property="og:image" content="'+$imgSrcComplete+'" />');
		});
		
	}
	$(document).ready(function() {
		OGfindMoreImages();
	});
</script></head>

